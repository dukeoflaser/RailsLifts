<h1><%= @user.name %>'s Profile</h1>

<%= link_to 'View All Programs', programs_path %>
<%= link_to 'View My Programs', my_programs_path %>

<hr>
<h2>Last Workout: <%= @last_workout.name %></h2>
<h3><%= @last_workout.created_at.strftime("%d %b. %Y")%></h3>
<%= render layout: 'layouts/exercise_table' do %>
  <% @last_workout.exercises.each do |exercise| %>
    <%= render partial: 'exercise_templates/exercise_template', locals: { exercise_template: exercise } %>
  <% end %>
<% end %>
<hr>
<%= link_to 'Next Workout', user_next_workout_path(current_user) %>
<%= link_to 'Workout History', user_workout_history_path(current_user) %>
